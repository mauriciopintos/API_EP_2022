     mkdir api_v1
     cd api_v1/
     npm install --save sequelize
     npm init -y
     sudo apt-get install mariadb
     sudo apt-get install mariadb-server
     npm install --save mariadb
     sudo npm install express-generator -g
     npm install 
     npm install sequelize --save
     npm install sequelize-cli --save
     npx sequelize init
     DEBUG=api:* npm start



     npx sequelize-cli model:generate --name carrera --attributes nombre:string
     npx sequelize db:migrate


Entidades principales
     npx sequelize-cli model:generate --name materia --attributes nombre:string,id_carrera:integer,cod_mat:integer
     npx sequelize-cli model:generate --name alumno --attributes nombre:string,apellido:string,id_alumno:integer
     npx sequelize-cli model:generate --name departamento --attributes nombre:string

Entidades intermedias/relaciones
     npx sequelize-cli model:generate --name profesor_materia --attributes id_profesor:integer,id_materia:integer
     npx sequelize-cli model:generate --name estudia --attributes id_alumno:integer,id_carrera:string
     npx sequelize-cli model:generate --name inscripcion_materia --attributes id_alumno:integer,id_materia:integer

